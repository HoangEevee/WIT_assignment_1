<h2 class="center">{{data.firstName}} {{data.lastName}}'s Health Record </h2>

{{!-- <h2 class = "center">Glucose Record</h2> --}}
<div class="tablecontainer">
    <table class="table">
        <thead>
            <tr>
                <th>Time</th>
                <th>Blood Glucose (mmol/L)</th> 
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
            {{#each data.glucoseTimestamp}}
            <tr>
                <td data label = "Time">{{this.time}}</td>
                <!--This red highlight for out of range does not work yet-->
                <td data label = "Blood Glucose (mmol/L)">{{this.value}}</td>
                <td data label = "Message">{{this.message}}</td> 
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>    